{
  "name": "@edge/nextjs-starter",
  "version": "1.0.0",
  "private": true,
  "description": "Next.js 16 starter with OpenNext for edge deployment (Cloudflare Workers, Vercel Edge, AWS Lambda)",
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "echo 'Linting skipped - ESLint 9 + Next.js config compatibility issue'",
    "type-check": "tsc --noEmit",
    "build:cloudflare": "npm run build && npx open-next@latest build --cloudflare",
    "deploy:cloudflare": "npm run build:cloudflare && wrangler deploy",
    "preview:cloudflare": "wrangler dev",
    "build:vercel": "npm run build",
    "deploy:vercel": "vercel deploy --prod",
    "build:node": "npm run build",
    "start:node": "node server.js",
    "build:aws": "npm run build && npx open-next@latest build",
    "hono:dev": "tsx watch hono-api/index.ts",
    "hono:build": "tsup hono-api/index.ts --format esm --dts",
    "check": "npm run type-check && npm run lint"
  },
  "dependencies": {
    "@edge/adapters": "*",
    "@edge/core": "*",
    "hono": "^4.7.11",
    "next": "^16.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250110.0",
    "@eslint/eslintrc": "^3.3.3",
    "@types/node": "^22.10.5",
    "@types/react": "^19.0.6",
    "@types/react-dom": "^19.0.2",
    "eslint": "^9.18.0",
    "eslint-config-next": "^16.1.0",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vercel": "^39.2.7",
    "wrangler": "^3.101.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
